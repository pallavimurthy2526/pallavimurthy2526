#include <stdio.h>

int main(){
   int a[10],i,j,temp,n;
   printf("enter no.of elements:");
   scanf("%d",&n);
   printf("enter elements:");
   for(i=0;i<n;i++){
      scanf("%d",&a[i]);
   }
   for(i=0;i<n;i++){
      for(j=i+1;j<n;j++){
         if(a[i]>a[j]){
            temp=a[i]; 
            a[i]=a[j];
            a[j]=temp;
         }
      }
   }
   printf("Elements are now in sequential order:\n");
   for(i=0;i<n;i++)
      printf("%d\t",a[i]);
   return 0;
}
